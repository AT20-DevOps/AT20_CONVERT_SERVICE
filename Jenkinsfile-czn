
pipeline{
    agent any
    stages{
        stage("Test"){
            agent { docker 'node:18-alpine3.16' }
            steps{
                sh 'npm version'
                sh 'ls -la'
                sh 'npm install ts-jest @types/jest --save-dev'
                sh 'npm install'
                sh 'npm test'
            }
        }
    }
}