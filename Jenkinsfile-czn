
pipeline{
    agent any
    stages{
        stage("Test"){
            agent { docker 'node:18-alpine3.16' }
            steps{
                //sh 'npm version'
                //sh 'ls -la'
                sh 'npm install --save-dev ts-jest'
                sh 'npm install --save-dev @types/jest'
                sh 'npm run test'
            }
        }
    }
}